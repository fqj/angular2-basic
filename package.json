{
  "name": "angular2-basic",
  "version": "1.0.0-beta2",
  "license": "MIT",
  "scripts": {
    "aot": "ngc -p ./tsconfig.aot.json",
    "build:pro": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run clean:aot && npm run aot && webpack --config config/webpack.aot.js --profile --bail",
    "build2:aot": "webpack --config config/webpack.aot.js --profile --bail",
    "build": "npm run clean:dist && npm run build:pro",
    "ci": "npm run lint && npm test && npm run e2e",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "protractor": "protractor config/protractor.conf.js",
    "test": "npm run test:pro",
    "test:pro": "karma start",
    "test:dev": "karma start --dev",
    "tslint": "tslint",
    "docs": "npm run typedoc -- --options config/typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs:compodoc": "npm run compodoc",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -d docs/compodoc/ -s -o ",
    "typedoc": "typedoc",
    "clean:aot": "npm run rimraf -- dist",
    "clean:dist": "npm run rimraf -- dist && npm run rimraf -- dist-aot",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "rimraf": "rimraf",
    "serve:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/ --inline --hot",
    "serve:pro": "npm run build:pro & http-server dist --cors -o",
    "serve": "npm run serve:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/platform-server": "2.4.3",
    "@angular/router": "3.4.3",
    "@angularclass/conventions-loader": "1.0.13",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "assets-webpack-plugin": "3.5.0",
    "core-js": "2.4.1",
    "hammerjs": "^2.0.8",
    "http-server": "0.9.0",
    "ie-shim": "0.1.0",
    "ng2-translate": "4.2.0",
    "offline-plugin": "4.5.4",
    "rxjs": "5.0.3",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/hammerjs": "2.0.34",
    "@types/jasmine": "2.5.40",
    "@types/node": "6.0.58",
    "@types/selenium-webdriver": "2.53.39",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "2.1.0",
    "angular2-router-loader": "0.3.4",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "codelyzer": "~2.0.0-beta.4",
    "compodoc": "0.0.30",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "file-loader": "0.9.0",
    "gh-pages": "0.12.0",
    "html-webpack-plugin": "2.26.0",
    "imports-loader": "0.7.0",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine-spec-reporter": "3.2.0",
    "json-loader": "0.5.4",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-phantomjs-shim": "1.4.0",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.1",
    "node-sass": "4.1.1",
    "open-browser-webpack-plugin": "0.0.3",
    "parse5": "3.0.1",
    "protractor": "4.0.14",
    "raw-loader": "0.5.1",
    "rimraf": "~2.5.4",
    "sass-loader": "4.1.1",
    "script-ext-html-webpack-plugin": "1.4.1",
    "source-map-loader": "0.1.6",
    "string-replace-loader": "1.0.5",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.2",
    "ts-node": "1.7.2",
    "tslint": "4.3.1",
    "tslint-eslint-rules": "3.2.3",
    "tslint-loader": "3.3.0",
    "typescript": "2.1.1",
    "url-loader": "0.5.7",
    "v8-lazy-parse-webpack-plugin": "0.3.0",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "2.4.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
